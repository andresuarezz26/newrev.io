{
  "name": "aider-browser-client",
  "version": "0.1.0",
  "description": "NewRev - Desktop UI for Aider AI pair programming",
  "author": "NewRev Team",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.13.0",
    "axios": "^1.4.0",
    "lucide-react": "^0.475.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "wait-on": "^7.0.0",
    "nodemon": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm run build && cross-env NODE_ENV=development electron .",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && cross-env ELECTRON_USE_DEV_SERVER=true electron .\"",
    "electron-dev-live": "concurrently \"npm start\" \"wait-on http://localhost:3000 && cross-env ELECTRON_USE_DEV_SERVER=true nodemon --exec electron . --watch public --watch src\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-pack-unsigned": "npm run build && electron-builder --config.mac.identity=null",
    "preelectron-pack": "npm run build",
    "start:web": "react-scripts start",
    "dev:hybrid": "concurrently \"npm run start:web\" \"wait-on http://localhost:3000 && cross-env ELECTRON_USE_DEV_SERVER=true nodemon --exec electron . --watch public --watch src\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.newrev.aider",
    "productName": "NewRev",
    "directories": {
      "output": "dist-deps-check"
    },
    "files": [
      "build/**/*",
      "public/electron.js",
      "public/preload.js",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "../api",
        "to": "api"
      },
      {
        "from": "../aider", 
        "to": "aider"
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "identity": null,
      "target": [
        {
          "target": "dmg",
          "arch": ["arm64", "x64"]
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "afterSign": null,
    "afterAllArtifactBuild": null
  }
}
